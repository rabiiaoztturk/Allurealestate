        <style>
            /* Swiper Container */
            .swiper-container {
                width: 100%;
                height: 100vh; /* Sayfanın yüksekliği kadar olmasını sağlıyor */
                overflow: hidden;
                position: relative;
            }

            /* Swiper Slide */
            .swiper-slide {
                background-size: cover;
                background-position: center;
                height: 100%;
                display: flex;
                align-items: center;
                justify-content: center;
                z-index: 0; /* Her slayt arka planda */
                position: relative;
                opacity: 0;
                visibility: hidden;
                transition: opacity 0.5s, visibility 0.5s;
            }

            .swiper-slide-active {
                opacity: 1;
                visibility: visible;
            }

            /* Yazıları sola hizala */
            .jumbotron-classic-content {
                text-align: left; /* Solda hizalama */
                max-width: 800px;
                margin-left: 20px; /* İçeriğin biraz boşlukla başlaması */
                color: white;
                z-index: 1; /* İçeriği üstte tutar */
                position: relative;
            }

                /* Başlık için sola hizalama */
                .jumbotron-classic-content h1,
                .jumbotron-classic-content p {
                    text-align: left;
                }

            .button-outer a {
                text-decoration: none !important; /* Alt çizgi yok */
                padding: 10px 20px !important; /* İç boşluk */
                background-color: rgba(255, 255, 255, 0.4) !important; /* Şeffaf beyaz arka plan */
                color: black !important;
                /* Siyah yazı */
                border: 2px solid #4a4a4a !important;
                 /* Siyah kenarlık */
                border-radius: 5px !important;
                 /* Hafif yuvarlak kenarlar */
                transition: background-color 0.3s, color 0.3s, transform 0.2s; /* Geçiş efektleri */
            }

                .button-outer a:hover {
                    background-color: rgba(0, 0, 0, 0.7) !important; /* Şeffaf siyah */
                    color: white !important; /* Yazı beyaz */
                    border-color: #6c6c6c !important; /* Hover durumunda daha açık koyu gri */
                    transform: scale(1.1); /* Hafif büyüme efekti */
                }



            /* Swiper Pagination */
            .swiper-pagination-modern {
                bottom: 20px;
                text-align: center;
            }

                .swiper-pagination-modern .swiper-pagination-bullet {
                    background: white;
                    opacity: 0.7;
                }

                .swiper-pagination-modern .swiper-pagination-bullet-active {
                    background: #007bff;
                    opacity: 1;
                }
        </style>
        <script>
            var swiper = new Swiper('.swiper-container', {
                loop: true,
                effect: 'fade', // Fade efektini etkinleştirir
                fadeEffect: {
                    crossFade: true, // Slaytlar arası geçişte içeriği temizler
                },
                autoplay: {
                    delay: 4759,
                },
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true,
                },
            });

        </script>
